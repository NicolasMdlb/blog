{% extends 'base.html.twig' %}

{% block title %}{{ category.name }}{% endblock %}

{% block stylesheets %}{% endblock %}

{% block body %}

    <h1>All categories </h1>
    {% for article in articles %}
        <div>
            <h1>{{ article.id }}</h1>
            <h5>Real article s title : {{ article.title }}</h5>
            <p>{{ article.content }}</p>
        </div>
    {% endfor %}
    <a href="{{ path('homepage') }}">
        Back to homepage.
    </a>

{% endblock %}